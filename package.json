    "lint": "npx eslint src --ignore-pattern '.js,.ts'",
    "fix": "npx eslint src --fix",
    "format": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "forfix": "npx prettier --write src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
